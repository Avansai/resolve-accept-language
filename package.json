{
  "name": "resolve-accept-language",
  "version": "1.1.14",
  "description": "Resolve the preferred locale based on the value of an `Accept-Language` HTTP header.",
  "author": "Avansai (https://avansai.com)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Avansai/resolve-accept-language.git"
  },
  "main": "lib/resolve-accept-language.js",
  "types": "lib/resolve-accept-language.d.ts",
  "scripts": {
    "build": "npm run prettier && npm run lint && rm -Rf ./lib && tsc && npm test",
    "lint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx --fix .",
    "lint-print-config": "eslint --print-config ./eslintrc.yaml",
    "prettier": "prettier --write .",
    "test": "jest --coverage",
    "release": "dotenv -- release-it --only-version"
  },
  "license": "MIT",
  "files": [
    "lib"
  ],
  "keywords": [
    "accept-language",
    "RFC 4647",
    "locale",
    "language tags",
    "RFC 4646",
    "BCP 47",
    "RFC 2616",
    "resolve",
    "detect",
    "intl",
    "i18n",
    "internationalization"
  ],
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.0.0",
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.1",
    "prettier-plugin-sh": "^0.12.8",
    "release-it": "^15.2.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
