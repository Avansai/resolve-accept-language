    "lint-check": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx --ext .json .",
    "lint-fix": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx --ext .json --fix .",
    "lint-print-config": "eslint --print-config ./eslintrc.yaml",
